<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" 
			"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="dialect">org.hibernate.dialect.MySQLDialect</property>
		<property name="show_sql">true</property>
		<mapping resource="org/galilee/dms/model/Sites.hbm.xml" />
		<mapping resource="org/galilee/dms/model/Rivers.hbm.xml" />
		<mapping resource="org/galilee/dms/model/Sources.hbm.xml" />
		<mapping resource="org/galilee/dms/model/Features.hbm.xml" />
		<mapping resource="org/galilee/dms/model/Fishes.hbm.xml" />
		<mapping resource="org/galilee/dms/model/FishCharacterCV.hbm.xml" />
		<mapping resource="org/galilee/dms/model/Variables.hbm.xml" />
		<mapping resource="org/galilee/dms/model/Units.hbm.xml" />
		<mapping resource="org/galilee/dms/model/VariableNameCV.hbm.xml" />
		<mapping resource="org/galilee/dms/model/ValueTypeCV.hbm.xml" />
		<mapping resource="org/galilee/dms/model/Methods.hbm.xml" />
		<mapping resource="org/galilee/dms/model/SiteCode.hbm.xml" />
		<mapping resource="org/galilee/dms/model/DataValues.hbm.xml" />
	</session-factory>
</hibernate-configuration>